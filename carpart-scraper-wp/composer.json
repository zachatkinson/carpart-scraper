{
  "name": "csf/parts-catalog",
  "description": "CSF Parts Catalog WordPress Plugin",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "CSF Parts Team"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "yoast/phpunit-polyfills": "^2.0",
    "mockery/mockery": "^1.6",
    "brain/monkey": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "CSF_Parts\\": "includes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CSF_Parts\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test:coverage": "phpunit --coverage-html coverage/html",
    "test:unit": "phpunit --testsuite='Unit Tests'"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
